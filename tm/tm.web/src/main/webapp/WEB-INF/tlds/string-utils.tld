	<function>
		<name>truncateAndEllipse</name>
		<function-class>org.squashtest.tm.web.internal.helper.StringEllipser</function-class>
		<function-signature>java.lang.String truncateAndEllipse(java.lang.String, java.lang.Integer)</function-signature>
