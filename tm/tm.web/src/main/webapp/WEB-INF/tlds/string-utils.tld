	<function>
		<name>hyphenedToUnderscored</name>
		<function-class>org.squashtest.tm.service.internal.helper.HyphenedStringHelper</function-class>
		<function-signature>java.lang.String hyphenedToUnderscored(java.lang.String)</function-signature>
