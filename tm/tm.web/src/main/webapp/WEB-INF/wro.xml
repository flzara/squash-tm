	<!-- This group is used to produce squash's core css ie non-themed css -->
  <group name="squash.core">
    <css>/styles/structure.css</css>
    <css>/styles/ckeditor.override.css</css>
     <css>/styles/bootstrap.override.css</css>
		<!-- squashtree is loaded through ajax by jstree <css>/styles/squashtree.css</css> -->
  </group>

	<!-- These groups produce squash's various themed css -->
  <group name="squash.blue">
    <css>/styles/master.blue.less</css>
  </group>
  <group name="squash.green">
    <css>/styles/master.green.css</css>
  </group>
  <group name="squash.grey">
    <css>/styles/master.grey.css</css>
  </group>
  <group name="squash.purple">
    <css>/styles/master.purple.css</css>
  </group>
  <group name="squash.wine">
    <css>/styles/master.wine.css</css>
  </group>
  <group name="squash.blue.green">
    <css>/styles/master.blue-green.css</css>
  </group>
  <group name="squash.green.blue">
    <css>/styles/master.green-blue.css</css>
  </group>

	<!-- Groups of non concatenable css -->
  <group name="squash.tree">
    <css>/styles/squashtree.css</css>
  </group>
  <group name="squash.core.override">
    <css>/styles/structure.override.css</css>
  </group>
  <group name="squash.subpage.override">
    <css>/styles/structure.subpageoverride.css</css>
  </group>
  <group name="squash.print">
    <css>/styles/master.css</css>
    <css>/styles/print.css</css>
  </group>

	<!-- This group is used to lint stylesheets -->
  <group name="csslint">
    <css minimize="false">/styles/**/*.css</css>
  </group>

</groups>
